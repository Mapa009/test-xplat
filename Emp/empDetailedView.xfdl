			 Tab01.tabpage1.empImg.image="URL('http://localhost/EmpImg/"+empCode+'"."'+imgExtend+"')";  
			 var empImg=Tab01.tabpage1.empImg.image;
			 trace("부서명검색"+empImg);
		}
	  }
	  
	  
	if(trID=="searceCodeID"){ //셀클릭 콜백 
		 var empCode=ds_empList.getColumn(ds_empList.rowposition,"EMP_CODE");
		 var imgExtend=ds_empList.getColumn(ds_empList.rowposition,"IMG_EXTEND");
		   imgName=empCode+imgExtend;
		   // 다른 사원을 클릭했을시
		   Tab01.tabpage1.empImg.image="URL('http://localhost/EmpImg/"+empCode+'"."'+imgExtend+"')";
		   var empImg=Tab01.tabpage1.empImg.image;
		   trace(empImg);
	   }
	  
	  
	  if(trID=="searchEmpID"){ // 사원명 검색
		if(ErrorCode!=0){
			alert("검색실패"+ErrorMsg);
		}else{
		   var empList=ds_empList.rowcount;
		  if(empList==0){	// 그 부서의 등록된 사원이 없을경우
			 alert("해당 사원은 없습니다.");
			 Tab.page2.name_text.value=null;
		  }
			var empCode=ds_empList.getColumn(ds_empList.rowposition,"EMP_CODE");
			var imgExtend=ds_empList.getColumn(ds_empList.rowposition,"IMG_EXTEND");
			
		Tab01.tabpage1.empImg.image="URL('http://localhost/EmpImg/"+empCode+'"."'+imgExtend+"')";
		}
	  }
	  
	  
	  if(trID=="updateEmpID"){ // 저장버튼 콜백  
		if(ErrorCode!=0){
			alert("저장실패 : "+ErrorMsg);
		}else{ 
			alert("저장되었습니다.");
		 }
	   }
	  
	  
	  if(trID=="imgUpLoadID"){  // 사진 저장 콜백 
		if(ErrorCode!=0){
				alert("사진등록 실패 : "+ErrorMsg);
			}else{
				alert("사진등록 완료.");
			}
	 }
	}








