	//사원 삭제 
	function Tab_page1_btn_delete_onclick(obj:Button,  e:ClickEventInfo)
	{

	var count=ds_empList.rowcount;
	var num=0;

	  for(var i=0;i<count;i++){
		 var status=ds_empList.getColumn(i,"STATUS");
		 
			 if(status==1){
				 transaction(
							"deleteEMP",
							"svcEmp::removeEmployeeList",
							"gds_emp=ds_empList:u",
							"",
							"",
							"fn_delete"
							);
							num++;
					}
				}
				if(num==0){
				alert("삭제할 행이 없습니다.");
				return;
			}

	 }


